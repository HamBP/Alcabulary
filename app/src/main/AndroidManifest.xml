<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.algosketch.alcabulary">

    <application
        android:name=".mvvm.global.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyVocabulary">
        <activity android:name=".mvvm.feature.select.SelectVocabularyActivity"></activity>
        <activity android:name=".mvvm.feature.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mvvm.feature.example.StudyExampleActivity" />
        <activity android:name=".mvvm.feature.add.AddVocabularyActivity" />
        <activity android:name=".mvvm.feature.main.MainActivity"></activity>
    </application>

</manifest>